pipeline {

 agent any
 
 parameters {
  choice( 
         name: 'Environment',
         choices: ['Develop', Feature],
         description: 'Environment choices'
  )
 }
   tools {
     maven "Maven3"
   }
   
   options {
     timestamps()
     skipDefaultCheckout()
   }
   
   stages {
      stage('checkout') {
        steps {
          echo "checkout"
          git checkout []
        }
      }
   }
}